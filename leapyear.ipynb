   "source": [
    "year=int(input(\"enter a number :\"))\n",
    "if year%4==0 and year%100!=0:\n",
    "    print(year,\"is a leap year\")\n",
    "elif year%100==0:\n",
    "    print(year,\"is not a leap year\")\n",
    "elif year%400==0:\n",
    "    print(year,\"if a leap year\")\n",
    "else:\n",
    "    print(year,\"is not a leap year\")"
